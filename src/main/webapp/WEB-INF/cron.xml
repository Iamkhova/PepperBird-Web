<?xml version="1.0" encoding="UTF-8"?>
<cronentries>
  <cron>
    <url>/admin/rssparser?value=syncRSS</url>
    <description>Sync RSS Feeds to datastore for processing</description>
    <schedule>every day 01:00</schedule>
    <timezone>America/New_York</timezone>
  </cron>
  <cron>
    <url>/admin/rssparser?value=sync2Blog</url>
    <description>Sync DB RSS Feeds to Blogger</description>
    <schedule>every day 03:00</schedule>
    <timezone>America/New_York</timezone>
  </cron>
</cronentries>